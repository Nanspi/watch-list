<header>
  <div class="overlay">
    <h1>📽 🎬<strong>Save any kind of movies</strong>📽 🎬</h1>
    <br>
    <button><%= link_to "Create a movie list", new_list_path %></button>
  </div>
</header>

<div>
  <div class="cards">
    <% @lists.each do |list| %>
        <div class="card-category">
             <%= link_to list.name, list_path(list) %>
         </div>
    <% end %>
  </div>
</div>
